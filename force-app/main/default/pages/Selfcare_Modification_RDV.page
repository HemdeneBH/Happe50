<script type="text/javascript">
     function refreshSnippet() {
    var c = ("https:" == document.location.protocol ? "https://" : "http://") + "cdn.tagcommander.com/3468/tc_ENGIE_iframe.js";
    if (0 === $("script[src\x3d'" + c + "']").length)
